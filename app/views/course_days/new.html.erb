<div class="container">
	<div class="row">
		<div class="col-md-8 col-md-offset-2 col-sm-6 col-sm-offset-3">
			<% if session[:setup_wizard] %>	
				<%= render 'wizard_controls'%>
			<%end%>
	   	<div class="panel panel-default">
	  		<div class="panel-heading">
			    <h4 class="panel-title"><i class="fa fa-calendar"></i> New Day</h4>
				</div>
				<div class="panel-body">
					<%= render 'form' %>
				</div>
			</div>
		</div>
	</div>
</div>
	

<script type="text/javascript">
 <% unless @course_day.errors.any? %>
  $(document).ready(function() { $(".add_location").click() } )
  $(document).ready(function() { $(".add_time").click() } )
 <% end %>
</script>
